#include <iostream>

double subtractNumbers(double num1, double num2) {
    return num1 - num2;
}

int main() {
    double num1, num2, difference;
    std::cout << "Enter the first number: ";
    std::cin >> num1;
    std::cout << "Enter the second number: ";
    std::cin >> num2;
    difference = subtractNumbers(num1, num2);
    std::cout << "The difference is: " << difference << std::endl;
    return 0;
}
